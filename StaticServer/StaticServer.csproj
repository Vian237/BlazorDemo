<Project Sdk="Microsoft.NET.Sdk.Web">
  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <!-- Force this project to use only nuget.org to avoid private feeds (e.g., Azure DevOps) causing 401 errors -->
    <RestoreSources>https://api.nuget.org/v3/index.json</RestoreSources>
    <RestoreIgnoreFailedSources>true</RestoreIgnoreFailedSources>
  </PropertyGroup>
  <ItemGroup>
    <!-- Required to expose MudBlazor static web assets under /_content/MudBlazor -->
    <PackageReference Include="MudBlazor" Version="8.10.0" />
  </ItemGroup>
</Project>
